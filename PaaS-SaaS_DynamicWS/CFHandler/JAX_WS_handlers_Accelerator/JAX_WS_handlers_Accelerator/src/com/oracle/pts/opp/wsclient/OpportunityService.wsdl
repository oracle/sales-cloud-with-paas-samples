    <wsdl:documentation xmlns:oer="http://xmlns.oracle.com/oer">
        <name>Opportunity Service</name>
        <description>A service to create, update, get, find, or delete opportunities. An
            opportunity is used by a sales organization to track information
            about a potential sale such as the sales account, key contacts,
            product interests, and potential revenues. WARNING: This service
            will be deprecated and replaced with a newer version in the upcoming
            release.</description>
        <docCategories>
            <category>Integration</category>
        </docCategories>
        <oer:lifecycle>Active</oer:lifecycle>
        <oer:compatibility>Supported - Backward Compatibility Assured</oer:compatibility>
        <keywords>
            <value>Opportunity Service</value>
        </keywords>
        <oer:category name="lba">
            <value>SalesTop-Sales</value>
            <value>SalesTop-Sales : OpptyMgmtTop-Sales Opportunity Top</value>
            <value>SalesTop-Sales : OpptyMgmtTop-Sales Opportunity Top :
                OpptyMgmtOpportunities-Opportunities</value>
        </oer:category>
        <oer:category name="lbo">
            <value>CRM-CRM : MOO-Opportunity Management :
                MOO_OPPORTUNITY-Opportunity</value>
        </oer:category>
        <oer:operation name="getOpportunity">
            <description>Queries a single Opportunity record by primary key.</description>
            <oer:parameters>
                <oer:parameter name="optyId" description="System generated identifier for this Opportunity."/>
                <oer:return description="Opportunity that matches the inbound key attributes."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="createOpportunity">
            <description>Creates a single Opportunity record.</description>
            <oer:parameters>
                <oer:parameter name="opportunity" description="Opportunity to be created."/>
                <oer:return description="Created opportunity."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="updateOpportunity">
            <description>Updates a single Opportunity record.</description>
            <oer:parameters>
                <oer:parameter name="opportunity" description="Opportunity to be updated."/>
                <oer:return description="Updated opportunity."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunity">
            <description>Delete a single Opportunity record.</description>
            <oer:parameters>
                <oer:parameter name="opportunity" description="Opportunity to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="mergeOpportunity">
            <description>Updates a single Opportunity record if one exists; otherwise,
                create a new one.</description>
            <oer:parameters>
                <oer:parameter name="opportunity" description="Opportunity to be merged."/>
                <oer:return description="Merged opportunity."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="processOpportunity">
            <description>Performs a Create, Update, Delete, or Merge operation on a list
                of Opportunity rows. The specified operation is applied to all
                objects in the given list.</description>
            <oer:parameters>
                <oer:parameter name="changeOperation" description="Indicates what action shall be performed, such as Create, Update, Merge, or Delete."/>
                <oer:parameter name="opportunity" description="A list of opportunities to be processed."/>
                <oer:parameter name="processControl" description="Controls the bulk upload behavior, such as whether the valid data will be committed if there are other invalid data in the same batch, and whether the outbound will contain the key attributes only or the full object."/>
                <oer:return description="A list of opportunities that are successfully processed."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="processCSOpportunity">
            <description>Performs a Create, Update, or Delete operation on a list of
                Opportunity rows. Different operations may be applied to
                different objects, depending on what is specified in the
                ChangeSummary object.</description>
            <oer:parameters>
                <oer:parameter name="processData" description="The object that is used to represent the changes that have been made."/>
                <oer:parameter name="processControl" description="Control the bulk upload behavior, such as whether the valid data will be committed if there are other invalid data in the same batch, and whether the outbound will contain the key attributes only or the full object."/>
                <oer:return description="Result of the operation including a list of created/updated data objects, and all warning and informational messages."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityCompetitor">
            <description>Deletes an Opportunity Competitor.</description>
            <oer:parameters>
                <oer:parameter name="opportunityCompetitorVO" description="Opportunity competitor to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityLead">
            <description>Deletes an Opportunity Lead.</description>
            <oer:parameters>
                <oer:parameter name="opportunityLeadVO" description="Opportunity lead to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityReference">
            <description>Deletes an Opportunity Reference.</description>
            <oer:parameters>
                <oer:parameter name="opportunityReferenceVO" description="Opportunity reference to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityResource">
            <description>Deletes an Opportunity Resource.</description>
            <oer:parameters>
                <oer:parameter name="opportunityResourceVO" description="Opportunity resource to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityMktResponse">
            <description>Deletes an Opportunity Response.</description>
            <oer:parameters>
                <oer:parameter name="opportunityResponseVO" description="Opportunity response to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityRevenue">
            <description>Deletes an Opportunity Revenue.</description>
            <oer:parameters>
                <oer:parameter name="opportunityRevenueVO" description="Opportunity revenue to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunitySource">
            <description>Deletes an Opportunity Source.</description>
            <oer:parameters>
                <oer:parameter name="opportunitySourceVO" description="Opportunity source to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="doAutoAssignmentAsynchronously">
            <description>Method which is called from auto assignment composite to assign
                a specific opportunity automatically when the opportunity is
                created or updated. WARNING: The operation will be renamed in
                the newer version of this service planned in the upcoming
                release.</description>
            <oer:parameters>
                <oer:parameter name="optyId" description="Opportunity identifier."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="deleteOpportunityContact">
            <description>Deletes an Opportunity Contact.</description>
            <oer:parameters>
                <oer:parameter name="opportunityContactVO" description="Opportunity contact to be deleted."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="findOpportunity">
            <description>Queries a group of Opportunities based on filter criteria.</description>
            <oer:parameters>
                <oer:parameter name="findCriteria" description="The criteria used to limit the query result, such as the filter (where clause), the fetch size, etc."/>
                <oer:parameter name="findControl" description="Not currently used."/>
                <oer:return description="A list of opportunities that satisfy the inbound criteria."/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="isSalesAccountUsedInOpportunity">
            <description>Method to determine if a given sales account has any
                opportunties.</description>
            <oer:parameters>
                <oer:parameter name="salesAccountId" description="Sales account party identifier."/>
                <oer:return description="Whether the sales account is associated with any opportunities"/>
            </oer:parameters>
        </oer:operation>
        <oer:operation name="checkConstraint">
            <description>This operation is an internal operation and is not superseded by
                another operation.</description>
            <oer:lifecycle>Deprecated</oer:lifecycle>
            <oer:parameters>
                <oer:parameter name="deleteEntities" description="This operation is an internal operation and is not superseded by another operation."/>
                <oer:return description="This operation is an internal operation and is not superseded by another operation."/>
            </oer:parameters>
        </oer:operation>
    </wsdl:documentation>

<?xml version = '1.0' encoding = 'UTF-8'?>
<oracle-webservice-clients xmlns:ns2="http://java.sun.com/xml/ns/javaee">
   <webservice-client>
      <service-qname namespaceURI="http://xmlns.oracle.com/apps/cdm/foundation/parties/locationService/applicationModule/"
                     localpart="LocationService"/>
      <port-info>
         <wsdl-port namespaceURI="http://xmlns.oracle.com/apps/cdm/foundation/parties/locationService/applicationModule/"
                    localpart="LocationServiceSoapHttpPort"/>

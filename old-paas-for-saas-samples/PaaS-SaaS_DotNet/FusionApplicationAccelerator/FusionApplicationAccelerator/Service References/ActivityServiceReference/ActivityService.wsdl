      <oer:lifecycle>Active</oer:lifecycle>
      <oer:parameters>
        <oer:parameter name="activity" description="Activity record to be created." />
        <oer:return description="Created Activity record." />
      </oer:parameters>
    </oer:operation>
    <oer:operation name="updateActivity">
